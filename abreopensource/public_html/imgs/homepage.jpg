// Function used to open new pages on the international gateway select box
function openPage(theform)
{
	// grab the value from the form element
	var newPage	=	theform.selCountry.value;
	// if the selected value is NOT "Select a Country" then open the new
	// page based on the value passed into the function.
	if(newPage	!=	"Select a Country")
	{
		//open a new pop up window.
		window.open(newPage);
	}
}

